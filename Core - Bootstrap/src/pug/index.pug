extends layouts/layout

block content
  .page.pt-5.px-4.pt-sm-6.px-sm-5.pt-xl-7.px-xl-7
    .page-head
      .h3.mb-4.mb-xl-5 Dashboard
    .page-body
      .row.g-0
        .col-lg-8.col-12.pe-lg-2
          .card.mb-2.p-4.p-sm-5
            .card-head.d-flex.align-items-center.justify-content-between.mb-5.mb-sm-6
              .title.title-color.red Overview
              select.select.select-small
                option All time
                option In a year
                option Per month
            .card-body.p-0
              .overview.js-animate-tabs.p-2
                .tabs.nav.flex-nowrap
                  .tab#overview-1.d-xs-block.d-sm-flex(data-bs-target="#overview-item-1" data-bs-toggle="tab")
                    .overview-icon.d-flex.justify-content-center.align-items-center.bg-light-blue
                      +i('shopping-bag-stroke')
                    .overview-details
                      .caption.d-flex.align-items-center.mb-1
                        | Customers
                        +info()(class='ms-1')
                      .h2.counter.mb-auto 1024
                    +balance('37.8%', 'down', true)(class="ms-auto")

                  .tab#overview-2.d-xs-block.d-sm-flex(data-bs-target="#overview-item-2" data-bs-toggle="tab")
                    .overview-icon.d-flex.justify-content-center.align-items-center.bg-light-purple
                      +i('activity')
                    .overview-details
                      .caption.d-flex.align-items-center.mb-1
                        | Income
                        +info()(class='ms-1')
                      .h2.counter.mb-auto 256k
                    +balance('38.2%', 'up', true)(class="ms-auto")
                  .bg-tab-active

              .overview-container.tab-content
                .tab-pane.overview-item#overview-item-1.fade.active.show(role="tabpanel" aria-labelledby="overview-1")
                  .users
                    .users-head.d-flex.align-items-center.justify-content-between.mb-4
                      .users-info
                        | Welcome <b>857 customers</b> with a personal message ðŸ˜Ž
                      a.btn-stroke(href="message-center.html")
                       | Send <span class="ms-1 d-none d-sm-block" >message</span>
                    .users-list.d-flex.justify-content-around
                      a.users-item.text-center(href="message-center.html")
                        .userpic
                          img(src='img/content/user-2.jpg')
                        .name Gladyce
                      a.users-item.text-center(href="message-center.html")
                        .userpic
                          img(src='img/content/user-3.jpg')
                        .name Elbert
                      a.users-item.text-center(href="message-center.html")
                        .userpic
                          img(src='img/content/user-4.jpg')
                        .name Joyce
                      a.users-item.text-center.d-none.d-sm-block(href="customer-list.html")
                        .icon-all.d-flex.align-items-center.justify-content-center
                          +i('arrow')
                        .name View all

                .tab-pane.overview-item#overview-item-2.fade(role="tabpanel" aria-labelledby="overview-2")
                  .chart.chart-overview-income
                    #overview-income

          .card.mb-2.p-4.p-sm-5
            .card-head.d-flex.align-items-center.justify-content-between.mb-5.mb-sm-6
              .title.title-color.purple Product views
              select.select.select-small
                option Last 7 days
                option All time
                option In a year
                option Per month
            .chart.chart-product-views
              #product-views

          .card.mb-2.p-4.p-sm-5
            .card-head.d-flex.align-items-center.justify-content-between.mb-5.mb-sm-6
              .title.title-color.green Product views
            .tips
              .card-info.mb-4 Need some ideas for the next product?
              .tips-list.row.g-0
                .col-12.col-sm-6.pe-lg-2
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('schedule')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Early access
                      .caption-info.d-flex
                        .badge.min.purple.me-1 New
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-1.jpg", alt="")
                          .time.pe-2 3 mins read
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('design')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Exclusive downloads
                      .caption-info.d-flex
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-2.jpg", alt="")
                          .time.pe-2 3 mins
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('phone')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Asset use guidelines
                      .caption-info.d-flex
                        .badge.min.green.me-1 Popular
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-3.jpg", alt="")
                          .time.pe-2 Time
                .col-12.col-sm-6.pe-lg-2
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('arrow')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Asset use guidelines
                      .caption-info.d-flex
                        .badge.min.green.me-1 Small label
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-4.jpg", alt="")
                          .time.pe-2 Time
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('video-recorder')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Behind the scenes
                      .caption-info.d-flex
                        .badge.min.red.me-1 Hot
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-5.jpg", alt="")
                          .time.pe-2 3 mins read
                  .tips-item.d-flex.align-items-center(data-bs-toggle="modal" data-bs-target="#modal-video")
                    .tips-icon.d-flex.align-items-center.justify-content-center.me-3.flex-shrink-0
                      +i('multiselect')
                    .tips-details.flex-shrink-0
                      .tips-title.mb-2 Life & work updates
                      .caption-info.d-flex
                        .user.d-flex.align-items-center
                          .userpic.me-1
                            img(src="img/content/user-6.jpg", alt="")
                          .time.pe-2 3 mins read

          .card.mb-2.p-4.p-sm-5
            .card-head.d-flex.align-items-center.justify-content-between.mb-5.mb-sm-6
